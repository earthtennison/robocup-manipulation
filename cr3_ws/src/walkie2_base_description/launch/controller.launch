<launch>

<rosparam file="$(find walkie2_base_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="walkie2_base" args="Rev2_position_controller Rev3_position_controller Slider12_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/walkie2_base/joint_states"/>
</node>

</launch>