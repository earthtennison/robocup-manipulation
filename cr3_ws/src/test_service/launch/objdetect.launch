<launch>
    <!-- realsense -->
    <include file="$(find realsense2_camera)/launch/rs_rgbd.launch">
        <arg name="align_depth" value="true" />
        <arg name="color_width" value="1280" />
        <arg name="color_height" value="720" />
        <arg name="color_fps" value="30" />
        <arg name="depth_width" value="1280" />
        <arg name="depth_height" value="720" />
        <arg name="depth_fps" value="30" />
        <arg name="filters" value="pointcloud"/>
    </include>

    <node pkg="tf2_ros" type="static_transform_publisher" name="camera_link_to_object_frame" args="0 0 0 0 1.57 0 camera_link object_frame"/>

    <!-- launch robot control node for moveit motion planning -->
    <!-- <node pkg="test_service" type="objdetect_server.py" name="objdetect_server" output="screen" /> -->

</launch>